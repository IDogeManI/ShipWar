<Window x:Class="FrontShipWar.GameWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FrontShipWar"
        mc:Ignorable="d"
        Title="GameWindow" Height="800" Width="400">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="229*"></RowDefinition>
            <RowDefinition Height="10"></RowDefinition>
            <RowDefinition Height="229*"></RowDefinition>
        </Grid.RowDefinitions>
        <UniformGrid Grid.Row="2" Rows="10" Columns="10" Name="grid_My" Loaded="grid_My_Loaded"/>
        <UniformGrid Grid.Row="0" Rows="10" Columns="10" Name="grid_Other" Loaded="grid_Other_Loaded"/>
    </Grid>
</Window>
